{
   "name": "redis-graphql-server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/jest": "^25.1.4",
      "@types/node": "^12.12.31",
      "@types/uuid": "^7.0.2",
      "gql2ts": "^1.10.1",
      "jest": "^25.2.3",
      "ts-jest": "^25.2.1",
      "ts-node": "^8.8.1",
      "tslint": "^6.1.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "^3.8.3"
   },
   "dependencies": {
      "@gql2ts/types": "^1.9.0",
      "@types/bcryptjs": "^2.4.2",
      "bcryptjs": "^2.4.3",
      "graphql-import": "^1.0.1",
      "graphql-request": "^1.8.2",
      "graphql-yoga": "^1.18.3",
      "npm-check-updates": "^4.1.0",
      "pg": "^7.18.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.22",
      "uuid": "^7.0.2"
   },
   "scripts": {
      "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
      "test": "NODE_ENV=test jest",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
   },
   "jest": {
      "globalSetup": "./src/testUtils/callSetup.js",
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
